<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="contactDetailsAnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.fidiym.contacts.controllers.ContactDetailsController">
   <children>
      <ImageView fitHeight="140.0" fitWidth="160.0" layoutX="30.0" layoutY="70.0" pickOnBounds="true" preserveRatio="true" />
      <AnchorPane fx:id="contactBuilderPane" layoutX="230.0" layoutY="30.0" prefHeight="250.0" prefWidth="350.0" style="-fx-background-color: #eee;" />
      <Button layoutX="90.0" layoutY="310.0" mnemonicParsing="false" onAction="#back_to_contacts" prefHeight="50.0" prefWidth="100.0" text="Back">
         <font>
            <Font name="Calibri" size="20.0" />
         </font>
      </Button>
      <Button layoutX="250.0" layoutY="310.0" mnemonicParsing="false" onAction="#apply_changes" prefHeight="50.0" prefWidth="100.0" text="Apply">
         <font>
            <Font name="Calibri" size="20.0" />
         </font>
      </Button>
      <Button layoutX="410.0" layoutY="310.0" mnemonicParsing="false" onAction="#remove_contact" prefHeight="50.0" prefWidth="100.0" text="Remove">
         <font>
            <Font name="Calibri" size="20.0" />
         </font>
      </Button>
      <Line endX="100.0" endY="20.0" layoutX="119.0" layoutY="30.0" startX="100.0" startY="230.0" stroke="#ababab" />
      <Label alignment="CENTER" layoutX="35.0" layoutY="222.0" prefHeight="30.0" prefWidth="150.0" text="Label">
         <font>
            <Font name="Calibri" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
